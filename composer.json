{
  "name": "melihovv/laravel-starter-kit",
  "description": "Laravel starter kit",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": ">=7.1.3",
    "ext-mbstring": "*",
    "ext-openssl": "*",
    "ext-pdo": "*",
    "ext-tokenizer": "*",
    "ext-xml": "*",
    "fideloper/proxy": "^4.0",
    "laravel/framework": "5.8.*",
    "laravel/tinker": "^1.0",
    "lorisleiva/laravel-actions": "^0.1.2",
    "melihovv/laravel-env-validator": "5.8.*"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "^3.2",
    "barryvdh/laravel-ide-helper": "^2.6",
    "beyondcode/laravel-dump-server": "^1.0",
    "doctrine/dbal": "^2.6",
    "filp/whoops": "^2.0",
    "fzaninotto/faker": "^1.4",
    "garygreen/pretty-routes": "^1.0.4",
    "laravel/telescope": "^2.0",
    "melihovv/laravel-package-generator": "5.8.*",
    "mockery/mockery": "^1.0",
    "nunomaduro/collision": "^3.0",
    "phpunit/phpunit": "^7.5",
    "sebastian/phpcpd": "^4.0",
    "squizlabs/php_codesniffer": "^3.0"
  },
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/adamwathan/laravel-preset"
    }
  ],
  "autoload": {
    "classmap": [
      "database/seeds",
      "database/factories"
    ],
    "psr-4": {
      "App\\": "app/"
    },
    "files": [
      "app/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "dont-discover": [
      ]
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
      "@php -r \"touch('.tinker');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan post-update-cmd",
      "@php artisan package:discover --ansi"
    ],
    "phpunit": "phpunit",
    "phpcs": "phpcs --standard=./phpcs-rules.xml --encoding=utf-8 app/",
    "phpcbf": "phpcbf --standard=./phpcs-rules.xml --encoding=utf-8 app/",
    "phpcpd": "phpcpd app/"
  },
  "config": {
    "platform": {
        "php": "7.1.3"
    },
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
