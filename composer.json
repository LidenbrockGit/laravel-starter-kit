{
  "name": "melihovv/laravel-starter-kit",
  "description": "Laravel starter kit",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": "^7.2",
    "ext-pdo": "*",
    "ext-tokenizer": "*",
    "ext-xml": "*",
    "beyondcode/laravel-self-diagnosis": "^1.3",
    "fideloper/proxy": "^4.0",
    "laravel/framework": "^6.2",
    "laravel/tinker": "^2.0",
    "lorisleiva/laravel-actions": "^0.2"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "^3.2",
    "barryvdh/laravel-ide-helper": "^2.6",
    "beyondcode/laravel-dump-server": "^1.3",
    "doctrine/dbal": "^2.10",
    "facade/ignition": "^1.16",
    "fzaninotto/faker": "^1.4",
    "garygreen/pretty-routes": "^1.0.7",
    "laravel/telescope": "^2.0",
    "laravel/ui": "^1.0",
    "melihovv/laravel-env-validator": "^6.0",
    "melihovv/laravel-package-generator": "^6.0",
    "mockery/mockery": "^1.0",
    "nunomaduro/collision": "^3.0",
    "phpunit/phpunit": "^8.0",
    "sebastian/phpcpd": "^4.0",
    "spatie/laravel-web-tinker": "^1.6",
    "squizlabs/php_codesniffer": "^3.5"
  },
  "autoload": {
    "classmap": [
      "database/seeds",
      "database/factories"
    ],
    "psr-4": {
      "App\\": "app/"
    },
    "files": [
      "app/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "dont-discover": [
      ]
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
      "@php -r \"touch('.tinker');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan post-update-cmd",
      "@php artisan package:discover --ansi"
    ],
    "phpunit": "phpunit",
    "phpcs": "phpcs --standard=./phpcs-rules.xml --encoding=utf-8 app/",
    "phpcbf": "phpcbf --standard=./phpcs-rules.xml --encoding=utf-8 app/",
    "phpcpd": "phpcpd app/"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "platform": {
        "php": "7.4.1"
    },
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
